<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- new StoreLetter() -->
	<bean id="store" class="dev.store.StoreNumber"></bean>
	
	<bean id="action1" class="dev.ihm.PrintAppVersionAction"></bean>
	
	<bean id="action2" class="dev.ihm.StoreListAction">
		<constructor-arg ref="store"></constructor-arg>
	</bean>
	
	<bean id="menu" class="dev.ihm.TwoActionsMenu">
		<constructor-arg ref="action1"> </constructor-arg>
		<constructor-arg ref="action2"> </constructor-arg>
	</bean>

</beans>
